#include <iostream>
#include <cmath>

using namespace std;
int  binsear (int n,int var, int *arr) {
    int l = 0, r, mid, search;
    r = n - 1;
    while (l <= r) {
        mid = (l + r) / 2;
        search = arr[mid];
        if (search == var)
            return mid;
        if (search < var)
            l = mid + 1;
        else
            r = mid - 1;
    }
    return -1;
}

int main() {
    int var;
    int arr[] = {1, 6, 9, 16, 19};
    cin >> var;
    int n=sizeof (arr)/ sizeof (arr[0]);
    cout<<binsear(n,var, arr);
    return 0;
}
